        $("#login").click(function () {
            $("#login-dialog").addClass("show");
            $("#signup-dialog").addClass("hide");
        });
        // Sign Up
        $("#sign_up").click(function () {
            $("#login-dialog").addClass("hide");
            $("#signup-dialog").addClass("show");
        });
    });
</script>

